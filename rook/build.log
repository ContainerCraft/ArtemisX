helm repo add rook-release https://charts.rook.io/release && helm repo update

helm install rook-ceph --namespace rook-ceph --create-namespace rook-release/rook-ceph --values ./rook/operator-values.yml --set clusterName=rook-ceph --set crds.enabled=true
